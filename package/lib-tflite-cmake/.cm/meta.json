{
  "customize": {
    "extra_dir": "",
    "git_src_dir": "src",
    "install_env": {
      "PACKAGE_GIT": "YES",
      "PACKAGE_PATCH": "YES",
      "PACKAGE_SKIP_LINUX_MAKE": "YES",
      "PACKAGE_PATCH_LIST": "\\$<<ORIGINAL_PACKAGE_DIR>>$/patch.linux.variations/\\$<<PACKAGE_PATCH__PACKAGE>>$ \\$<<ORIGINAL_PACKAGE_DIR>>$/patch.linux.variations/\\$<<PACKAGE_PATCH__OFFSETOF>>$",
      "PACKAGE_PATCH__PACKAGE": "",
      "PACKAGE_PATCH__OFFSETOF": "",
      "PACKAGE_SUB_DIR": "src",
      "PACKAGE_URL": "https://github.com/tensorflow/tensorflow",
      "PACKAGE_VERSION": "default",
      "PACKAGE_LIB_RUY": "OFF",
      "PACKAGE_LIB_XNNPACK": "OFF"
    }
  },
  "deps": {
    "lib-xnnpack": {
      "enable_if_env": {
        "PACKAGE_LIB_XNNPACK": [ "ON" ]
      },
      "add_to_path": "no",
      "force_target_as_host": "yes",
      "local": "yes",
      "name": "XNNPACK lib",
      "skip_from_bat": "yes",
      "sort": 20,
      "tags": "lib,xnnpack"
    },
    "cmake": {
      "add_to_path": "no",
      "force_target_as_host": "yes",
      "local": "yes",
      "name": "CMake tool",
      "skip_from_bat": "yes",
      "sort": 20,
      "tags": "tool,cmake",
      "version_from": [
        3,
        16,
        0
      ]
    },
    "compiler": {
      "local": "yes",
      "name": "C++ compiler",
      "sort": 0,
      "tags": "compiler,lang-cpp"
    }
  },
  "end_full_path": {
    "linux": "lib/libtensorflow-lite.a"
  },
  "need_cpu_info": "yes",
  "only_for_host_os_tags": [
    "linux"
  ],
  "only_for_target_os_tags": [
    "linux"
  ],
  "package_extra_name": " (from sources, static)",
  "process_script": "install",
  "soft_uoa": "lib.tflite.static",
  "suggested_path": "lib-tflite-src-static",
  "tags": [
    "lib",
    "tensorflow",
    "lite",
    "tflite",
    "tensorflow-lite",
    "tensorflow-static",
    "vsrc",
    "vstatic",
    "via-cmake"
  ],
  "use_preprocess_scripts_from_another_entry": {
    "data_uoa": "fd44428fbc77f77f",
    "module_uoa": "84e27ad9dd12e734"
  },
  "use_scripts_from_another_entry": {
    "data_uoa": "cd9ccc74060b3d18",
    "module_uoa": "script"
  },
  "variations": {
    "rpi4": {
      "extra_env": {
        "EXTRA_CXXFLAGS": "-march=armv7-a+neon+vfpv4 -mfpu=neon-vfpv4 "
      }
    },
    "v2.3.99": {
      "extra_env": {
        "PACKAGE_GIT_CHECKOUT": "master",
        "PACKAGE_PATCH__PACKAGE": "v2.3.99_package.patch",
        "PACKAGE_PATCH__OFFSETOF": "v2.3.99_offsetof.patch",
        "PACKAGE_VERSION": "2.3.99"
      },
      "on_by_default": "yes"
    },
    "with.ruy": {
      "extra_env": {
        "PACKAGE_LIB_RUY": "ON"
      }
    },
    "with.xnnpack": {
      "extra_env": {
        "PACKAGE_LIB_XNNPACK": "ON",
        "EXTRA_FLAGS": "-I${CK_ENV_LIB_XNNPACK_INCLUDE}"
      }
    }
  }
}
