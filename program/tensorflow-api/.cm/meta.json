{
  "run_deps": {
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 5,
      "tags": "compiler,python"
    },
    "protobuf": {
      "local": "yes",
      "sort": 10,
      "name": "ProtoBuf host library 3.0.x",
      "tags": "lib,protobuf-host,v3,v3.0"
    },
    "tensorflow-api-model":{
      "local": "yes",
      "name": "TensorFlow Object recognition API model",
      "sort": 20,
      "tags": "model,tensorflowapimodel"
    },
    "lib-tensorflow": {
      "local": "yes",
      "name": "Tensorflow library",
      "sort": 40,
      "tags": "lib,tensorflow"
    }
  },
  "data_name": "TensorFlow API Models",
  "program": "yes",
  "process_in_tmp": "yes",
  "no_compile": "yes",
  "no_target_file": "yes",
  "run_cmds": {
    "default":{
      "ignore_return_code": "no",
      "name":"Evaluation Job",
      "sort":10,
      "run_time":{
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../eval.py --logtostderr --pipeline_config_path=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/$<<CK_ENV_MODEL_TENSORFLOW_API_PIPELINE_NAME>>$ --checkpoint_dir=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/train --eval_dir=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/eval"
      } 
    }
  },
  "run_vars": {
    "CK_SAVE_IMAGES": "NO"
  },
  "tags": [
    "tensorflow",
    "tensorflow-api",
    "object-detection"
  ]
}
